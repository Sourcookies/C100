#include <stdio.h>
int main() {
	int year, month, day=0,i,d;
	scanf_s("%d %d %d", &year, &month, &d);
	int a[13] = {0,31,28,31,30,31,30,31,31,30,31,30,31 };
		if ((year % 4 == 0 && year % 100 != 0) || (year % 400 == 0))
		{
			a[2] += 1;
	}
		for (i = 1; i < month; i++)
		{
			day += a[i];
		}
		     day+= d;
          printf("%d\n", day);

	      return 0;
	}
-----------------------------------------------------------------------------------------------
	#include <stdio.h>
int main() {
	int year, month, day=0,i,d;
	scanf_s("%d %d %d", &year, &month, &d);
	int a[12] = {31,28,31,30,31,30,31,31,30,31,30,31 };
		if ((year % 4 == 0 && year % 100 != 0) || (year % 400 == 0))
		{
			a[1] += 1;
	}
		for (i = 0; i < month-1; i++)
		{
			day += a[i];
		}
		     day+= d;
          printf("%d\n", day);

	      return 0;
	}
