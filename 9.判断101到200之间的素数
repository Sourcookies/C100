#include <stdio.h>
int main() {
	for (int i = 101; i <= 200; i++)
	{
		int isprime = 1;
		for (int j = 2; j <i; j++)
		{
			if (i % j == 0)
			{
				isprime = 0;
				break;
			}
		}
		if (isprime == 1)
		{    
			printf("%d\n", i);
		}
	}
	      return 0;
	}
  //判断素数的方法：用一个数分别去除2到sqrt(这个数)，如果能被整除， 则表明此数不是素数，反之是素数。
  //第二个循坏里j<i，不是j<200。
  //isprime要放在第一个循环里，不然第一次循环过后，isprime就一直为0。
----------------------------------------------------------------------------------------------
#include <stdio.h>
#include <math.h>
int main() {
	for (int i = 101; i <= 200; i++)
	{ 
		int j;
		for ( j = 2; j <=sqrt(i); j++)
		{
			if (i % j == 0)
			{
				break;
			}
		} 
			if (j >sqrt(i))
		{    
			printf("%d\n", i);
		}
	}
	      return 0;
	}
	//调用函数，头文件。平方根
