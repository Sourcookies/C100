#include <stdio.h>
int main() {
	char a,b,c;
	for (int a = 'x'; a <= 'z'; a++)
	{
		for (int b = 'x'; b <= 'z'; b++)
		{
			for (int c = 'x'; c <= 'z'; c++)
			{
				if (a != b && a != c && b != c && a != 'x' && c != 'x' && c != 'z')
				{
					printf("a和%c比，b和%c比，c和%c比\n", a, b, c);
				}
			}
		}
	}
	      return 0;
	}
